<!DOCTYPE html>
<html lang="en">
<head>
    <title>Karnal Travel</title>
    <meta name="viewport" content="width=device-width, initial-scale=1">
    <meta charset="utf-8">
    <link rel="icon" href="~/Templates/$RZ9MMT1.png" />
    <meta name="keywords" content="Grand Tour Responsive web template, Bootstrap Web Templates, Flat Web Templates, Android Compatible web template,
Smartphone Compatible web template, free webdesigns for Nokia, Samsung, LG, SonyEricsson, Motorola web design" />


    <!-- css files -->
    <link href="/Templates/FrontEnd/css/bootstrap.css" rel='stylesheet' type='text/css' /><!-- bootstrap css -->
    <link href="/Templates/FrontEnd/css/style.css" rel='stylesheet' type='text/css' /><!-- custom css -->
    <link href="/Templates/FrontEnd/css/font-awesome.min.css" rel="stylesheet"><!-- fontawesome css -->
    <!-- //css files -->

    <link href="/Templates/FrontEnd/css/css_slider.css" type="text/css" rel="stylesheet" media="all">

    <!-- google fonts -->
    <link href="//fonts.googleapis.com/css?family=Open+Sans:300,300i,400,400i,600,600i,700,700i,800,800i" rel="stylesheet">
    <link href="//fonts.googleapis.com/css?family=Raleway:100,100i,200,200i,300,300i,400,400i,500,500i,600,600i,700,700i,800,800i,900,900i" rel="stylesheet">
    <!-- //google fonts -->
    <script>
        function ShowDiv()
        {
            Document.getElementbyId("");
        }

    </script>

</head>
<body>

    <!-- header -->
    <header>
        <div class="container">
            <!-- nav -->
            <nav class="py-md-4 py-3 d-lg-flex">
                <div id="logo">
                    <h1 class="mt-md-0 mt-2"> <a href="@Url.Action("Index","Home")"><span><img src="~/Templates/$RZ9MMT1.png" width="40" /></span> Karnal Travel </a></h1>
                </div>
                <label for="drop" class="toggle"><span class="fa fa-bars"></span></label>
                <input type="checkbox" id="drop" />
                <ul class="menu ml-auto mt-1">

                    <li class="active"><a href="@Url.Action("Index","Home")">Home</a></li>
                    <li class=""><a href="@Url.Action("Index","AboutUs")">About Us</a></li>
                    <li class=""><a href="@Url.Action("Index","Information")">Information</a></li>


                    <li class="">
                        <a href="@Url.Action("Index","Contact")">
                            Contact Us
                        </a>
                    </li>

                    @if (Session["usertype"] != null)
                    {
                        <li class="booking"><a href="@Url.Action("Logout","Account")">Logout</a></li>

                    }
                    else
                    {
                        <li class="booking"><a href="@Url.Action("Login","Account")">Login</a></li>
                    }

                    <li>
                        @using (Html.BeginForm("Search", "Information"))
                        {


                            <input type="search" name="keyword" value="" class="fa-search" placeholder="search" style="background-color:transparent; border: double solid; margin-left:30px; color:white; height:35px; border-radius:5px; " />
                            <input type="submit" name="name" value="Search" class="fa-subscript" style="margin-left:10px; background-color: #047ffc; color:white; height:35px; border-radius:5px;" />


                        }

                    </li>
                    <li class="active"><a onclick="ShowDiv()"><span style="font-size:7px;"> Adv.Search</span></a></li>
                </ul>
            </nav>

            <!-- //nav -->
        </div>
    </header>
    <!-- //header -->
    <!-- banner -->
    @RenderBody()

    <!--footer -->
    <footer>
        <section class="footer footer_w3layouts_section_1its py-5">
            <div class="container py-lg-4 py-3">
                <div class="row footer-top">
                    <div class="col-lg-3 col-sm-6 footer-grid_section_1its_w3">
                        <div class="footer-title">
                            <h3>Address</h3>
                        </div>
                        <div class="footer-text">
                            <p>Location : sharah e faisal , Pakistan</p>
                            <p>Phone : +92 3012116588</p>
                            <p>Email : <a href="mailto:info@example.com">info@example.com</a></p>
                            <p>Fax : +12 534894364</p>
                        </div>
                    </div>
                    <div class="col-lg-3 col-sm-6 footer-grid_section mt-sm-0 mt-4">
                        <div class="footer-title">
                            <h3>About Us</h3>
                        </div>
                        <div class="footer-text">
                            <p>
                                Karnal Travels is a leading online travel management company because it hasalways been an art to provide better services.


                            </p>
                        </div>

                    </div>
                    <div class="col-lg-3 col-sm-6 mt-lg-0 mt-4 footer-grid_section_1its_w3">
                        <div class="footer-title">
                            <h3>Travel Places</h3>
                        </div>
                        <div class="row">
                            <ul class="col-6 links">
                                <li><a href="#choose" class="scroll">Mumbai,India </a></li>
                                <li><a href="#overview" class="scroll">Karachi,Pakistan </a></li>
                                <li><a href="#pricing" class="scroll">Lahour,Pakistan</a></li>
                            </ul>

                        </div>
                    </div>
                    <div class="col-lg-3 col-sm-6 mt-lg-0 mt-4 footer-grid_section_1its_w3">
                        <div class="footer-title">
                            <h3>Newsletter</h3>
                        </div>
                        <div class="footer-text">
                            <p>By subscribing to our mailing list you will always get latest news and updates from us.</p>
                        </div>
                    </div>
                </div>
            </div>
        </section>
    </footer>
    <!-- //footer -->
    <!-- copyright -->
    <div class="copyright py-3 text-center">
        <p>© 2019 Karnal Tour. All Rights Reserved </p>
    </div>
    <!-- //copyright -->
    <!-- move top -->
    <div class="move-top text-right">
        <a href="#home" class="move-top">
            <span class="fa fa-angle-up  mb-3" aria-hidden="true"></span>
        </a>
    </div>
    <!-- move top -->


</body>
</html>